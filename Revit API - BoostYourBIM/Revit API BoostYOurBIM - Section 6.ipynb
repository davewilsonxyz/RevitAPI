   "cell_type": "raw",
   "metadata": {},
   "source": [
    "public void S6_1_setUnits()\n",
    "{\n",
    "\tDocument doc = this.Document;\n",
    "\tUnits units = doc.GetUnits();\n",
    "\t\n",
    "\tFormatOptions foLength = units.GetFormatOptions(UnitType.UT_Length);\n",
    "\tfoLength.DisplayUnits = DisplayUnitType.DUT_MILLIMETERS;\n",
    "\tfoLength.UnitSymbol = UnitSymbolType.UST_MM;\n",
    "\tfoLength.Accuracy = 10;\n",
    "\t\n",
    "\tFormatOptions foVolume = units.GetFormatOptions(UnitType.UT_Volume);\n",
    "\tfoVolume.DisplayUnits = DisplayUnitType.DUT_CUBIC_METERS;\n",
    "\tfoVolume.UnitSymbol = UnitSymbolType.UST_NONE;\n",
    "\t\n",
    "\tunits.SetFormatOptions(UnitType.UT_Length, foLength);\n",
    "\tunits.SetFormatOptions(UnitType.UT_Volume, foVolume);\n",
    "\t\n",
    "\tusing(Transaction t = new Transaction(doc, \"set units\"))\n",
    "\t{\n",
    "\t\tt.Start();\n",
    "\t\tdoc.SetUnits(units);\n",
    "\t\tt.Commit();\n",
    "\t}\n",
    "\t\n",
    "}"
   ]
